{{ form_start(form) }}
    {{ form_row(form.name, {
        label: 'Nom'
    }) }}
    {{ form_row(form.firstName, {
        label: 'Prénom'
    }) }}
    {{ form_row(form.email) }}
    {{ form_row(form.phone, {
        label: 'Téléphone'
    }) }}
    
    <button type="submit" class="btn btn-success">Enregistrer</button>
{{ form_end(form) }}
